            else if(InventoryGet( Player, "TailStraps").Asset.Name=="TailStrap"){ Player.Tailset1 = "TailStrap";  Player.Tailset2 = "MouseTailStrap2"}
            else if(InventoryGet( Player, "TailStraps").Asset.Name=="WolfTailStrap1"){ Player.Tailset1 = "WolfTailStrap1";  Player.Tailset2 = "WolfTailStrap2"}
            else if(InventoryGet( Player, "TailStraps").Asset.Name=="WolfTailStrap2"){ Player.Tailset1 = "WolfTailStrap2";  Player.Tailset2 = "WolfTailStrap3"}
            else if(InventoryGet( Player, "TailStraps").Asset.Name=="WolfTailStrap3"){ Player.Tailset1 = "WolfTailStrap3";  Player.Tailset2 = "KittenTailStrap2"}
        }
        else{
             Player.Tailset1 = "non"
             Player.Tailset2 = "non"
        }}
    if(Placeholder == "off"){           //-------------------------------Wings Scan ------------------------------------
        if(InventoryGet( Player, "Wings")){
            if(InventoryGet( Player, "Wings").Asset.Name=="AngelFeather"){ Player.Wingsset1 = "AngelFeather";  Player.Wingsset2 = "AngelWings"}
            else if(InventoryGet( Player, "Wings").Asset.Name=="AngelWings"){ Player.Wingsset1 = "AngelWings";  Player.Wingsset2 = "AngelFeather"}
            else if(InventoryGet( Player, "Wings").Asset.Name=="BatWings"){ Player.Wingsset1 = "BatWings";  Player.Wingsset2 = "DevilWings"}
            else if(InventoryGet( Player, "Wings").Asset.Name=="DevilWings"){ Player.Wingsset1 = "DevilWings";  Player.Wingsset2 = "BatWings"}
            else if(InventoryGet( Player, "Wings").Asset.Name=="FairyWings"){ Player.Wingsset1 = "FairyWings";  Player.Wingsset2 = "FairyWings"}
            else if(InventoryGet( Player, "Wings").Asset.Name=="FallenAngelWings"){ Player.Wingsset1 = "FallenAngelWings";  Player.Wingsset2 = "SuccubusFeather"}
            else if(InventoryGet( Player, "Wings").Asset.Name=="SuccubusWings"){ Player.Wingsset1 = "SuccubusWings";  Player.Wingsset2 = "DevilWings"}
            else if(InventoryGet( Player, "Wings").Asset.Name=="SuccubusFeather"){ Player.Wingsset1 = "SuccubusFeather";  Player.Wingsset2 = "FallenAngelWings"}
                }
        else{
             Player.Wingsset1 = "non"
             Player.Wingsset2 = "non"
        }}
    if(Placeholder == "off"){           //-------------------------------Species Scan  ------------------------------------     Player.Race
    if(InventoryGet( Player, "HairAccessory2")){
        if((InventoryGet( Player, "HairAccessory2").Asset.Name=="BunnyEars1") || (InventoryGet( Player, "HairAccessory2").Asset.Name=="BunnyEars2")){ Player.Race = "Bunny"}
                else if((InventoryGet( Player, "HairAccessory2").Asset.Name=="CowHorns")){ Player.Race = "Cow"}
                else if((InventoryGet( Player, "HairAccessory2").Asset.Name=="ElfEars")){ Player.Race = "Elf"}
                else if((InventoryGet( Player, "HairAccessory2").Asset.Name=="FoxEars1") || (InventoryGet( Player, "HairAccessory2").Asset.Name=="FoxEars2") || (InventoryGet( Player, "HairAccessory2").Asset.Name=="FoxEars3")){ Player.Race = "Fox"}
                else if((InventoryGet( Player, "HairAccessory2").Asset.Name=="Horns") || (InventoryGet( Player, "HairAccessory2").Asset.Name=="Horns2") || (InventoryGet( Player, "HairAccessory2").Asset.Name=="Horns3") || (InventoryGet( Player, "HairAccessory2").Asset.Name=="BatWings")){ Player.Race = "Demon"}
                else if((InventoryGet( Player, "HairAccessory2").Asset.Name=="Ears1") || (InventoryGet( Player, "HairAccessory2").Asset.Name=="Ears2") || (InventoryGet( Player, "HairAccessory2").Asset.Name=="KittenEars1") || (InventoryGet( Player, "HairAccessory2").Asset.Name=="KittenEars2")){ Player.Race = "Kitten"}
                else if((InventoryGet( Player, "HairAccessory2").Asset.Name=="MouseEars1") || (InventoryGet( Player, "HairAccessory2").Asset.Name=="MouseEars2")){ Player.Race = "Mouse"}
                else if((InventoryGet( Player, "HairAccessory2").Asset.Name=="PonyEars1")){ Player.Race = "Pony"}
                else if((InventoryGet( Player, "HairAccessory2").Asset.Name=="PuppyEars1") || (InventoryGet( Player, "HairAccessory2").Asset.Name=="PuppyEars2")){ Player.Race = "Puppy"}
                else if((InventoryGet( Player, "HairAccessory2").Asset.Name=="RaccoonEars1")){ Player.Race = "Raccoon"}
                else if((InventoryGet( Player, "HairAccessory2").Asset.Name=="SuccubusHorns")){ Player.Race = "Succubus"}
                else if((InventoryGet( Player, "HairAccessory2").Asset.Name=="WolfEars1") || (InventoryGet( Player, "HairAccessory2").Asset.Name=="WolfEars2")){ Player.Race = "Wolf"}
            }
        else{
             Player.Race = "Human"
        }}
    if(Placeholder == "off"){           //-------------------------------Blush Scan  ------------------------------------
      if (InventoryGet(Player, "Blush").Property.Expression=="Low"){Player.BlushBase = "2"}
      else if (InventoryGet(Player, "Blush").Property.Expression=="Medium"){Player.BlushBase = "3"}
      else if (InventoryGet(Player, "Blush").Property.Expression=="High"){Player.BlushBase = "4"}
      else if (InventoryGet(Player, "Blush").Property.Expression=="VeryHigh"){Player.BlushBase = "5"}
      else if (InventoryGet(Player, "Blush").Property.Expression=="Extreme"){Player.BlushBase = "6"}
      else if (InventoryGet(Player, "Blush").Property.Expression=="ShortBreath"){Player.BlushBase = "7"}
        else {Player.BlushBase = "1"}
        }
    if(Placeholder == "off"){           //-------------------------------Eyebrows Scan  ------------------------------------
      if (InventoryGet(Player, "Eyebrows").Property.Expression=="Raised"){Player.EyebrowsBase = "2"}
      else if (InventoryGet(Player, "Eyebrows").Property.Expression=="Lowered"){Player.EyebrowsBase = "3"}
      else if (InventoryGet(Player, "Eyebrows").Property.Expression=="OneRaised"){Player.EyebrowsBase = "4"}
