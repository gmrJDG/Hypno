        }
    if(Placeholder == "off"){           //-------------------------------Eyes Scan  ------------------------------------
      if (InventoryGet(Player, "Eyes").Property.Expression=="Closed"){Player.EyesBase = "2"}
      else if (InventoryGet(Player, "Eyes").Property.Expression=="Dazed"){Player.EyesBase = "3"}
      else if (InventoryGet(Player, "Eyes").Property.Expression=="Shy"){Player.EyesBase = "4"}
      else if (InventoryGet(Player, "Eyes").Property.Expression=="Sad"){Player.EyesBase = "5"}
      else if (InventoryGet(Player, "Eyes").Property.Expression=="Horny"){Player.EyesBase = "6"}
      else if (InventoryGet(Player, "Eyes").Property.Expression=="Lewd"){Player.EyesBase = "7"}
      else if (InventoryGet(Player, "Eyes").Property.Expression=="VeryLewd"){Player.EyesBase = "8"}
      else if (InventoryGet(Player, "Eyes").Property.Expression=="Heart"){Player.EyesBase = "9"}
      else if (InventoryGet(Player, "Eyes").Property.Expression=="HeartPink"){Player.EyesBase = "10"}
      else if (InventoryGet(Player, "Eyes").Property.Expression=="LewdHeart"){Player.EyesBase = "11"}
      else if (InventoryGet(Player, "Eyes").Property.Expression=="LewdHeartPink"){Player.EyesBase = "12"}
