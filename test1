    else if (Player.Stage == 20){Player.Emote = "00 00 00 00 00 00 00 00 00 00 00"; Player.Looptimer = 100; Player.HypnoSpeachblocker = "on";Player.Stage = 20}
  }
  if  (Player.RPSTriggerhyph == 10){// Blindfold yourself
       if (Player.Stage == 0){Player.Emote  = "00 00 00 00 00 00 00 00 00 00 00"; Player.Looptimer = 100; Player.HypnoSpeachblocker = "off";}
    else if (Player.Stage == 1){Player.Emote  = "00 00 00 00 00 00 00 00 00 00 00"; Player.Looptimer = 100; Player.HypnoSpeachblocker = "off"; InventoryWear(Player, "FullBlindfold", "ItemHead", color)}
    else if (Player.Stage == 2){Player.Emote  = "00 00 00 00 00 00 00 00 00 00 00"; Player.Looptimer = 100; Player.HypnoSpeachblocker = "on";Player.Stage = 20}
  }
