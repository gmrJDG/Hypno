      else if (InventoryGet(Player, "Eyebrows").Property.Expression=="Angry"){Player.EyebrowsBase = "6"}
      else if (InventoryGet(Player, "Eyebrows").Property.Expression=="Soft"){Player.EyebrowsBase = "7"}
