  //---------------------------------------------------------------------------------------------------------------------------------------------------
  function Hypnosisrun() {
    if (Placeholder == "off") {    
    Player.Actvitytest = rpstext.search(/me/ig) ; 
    Player.Actvitytest1 = rpstext.search(/my/ig) ; }
    Player.Tuuut = Hypnothingi[0]
    Player.Hypnobodypart = Hypnothingi[1]
    Player.HypnoAct = Hypnothingi[2]
    if ((Player.Tuuut == "ChatSelf")){Player.Hypnotarget = Player}
  else {
    Letter = rpstext.split(' ')
    Player.TTarget1 = Letter[1]
    Player.TTarget2 = Letter[2]
    Player.TTarget3 = Letter[3]
    Player.TTarget4 = Letter[4]
    Player.TTarget5 = Letter[5]
    Player.TTarget6 = Letter[6]
    Player.TTarget7 = Letter[7]
    Player.TTarget8 = Letter[8]
    Player.TTarget9 = Letter[9]
    if ((ChatRoomCharacter[0]) && (ChatRoomCharacter[0] != Player)) {if ((ChatRoomCharacter[0].Name.toLowerCase() == Player.TTarget1) || (ChatRoomCharacter[0].Name.toLowerCase() == Player.TTarget2) || (ChatRoomCharacter[0].Name.toLowerCase() == Player.TTarget3) || (ChatRoomCharacter[0].Name.toLowerCase() == Player.TTarget4) || (ChatRoomCharacter[0].Name.toLowerCase() == Player.TTarget5) || (ChatRoomCharacter[0].Name.toLowerCase() == Player.TTarget6) || (ChatRoomCharacter[0].Name.toLowerCase() == Player.TTarget7) || (ChatRoomCharacter[0].Name.toLowerCase() == Player.TTarget8) || (ChatRoomCharacter[0].Name.toLowerCase() == Player.TTarget9)){Player.Hypnotarget = ChatRoomCharacter[0]}}
    if ((ChatRoomCharacter[1]) && (ChatRoomCharacter[1] != Player)) {if ((ChatRoomCharacter[1].Name.toLowerCase() == Player.TTarget1) || (ChatRoomCharacter[1].Name.toLowerCase() == Player.TTarget2) || (ChatRoomCharacter[1].Name.toLowerCase() == Player.TTarget3) || (ChatRoomCharacter[1].Name.toLowerCase() == Player.TTarget4) || (ChatRoomCharacter[1].Name.toLowerCase() == Player.TTarget5) || (ChatRoomCharacter[1].Name.toLowerCase() == Player.TTarget6) || (ChatRoomCharacter[1].Name.toLowerCase() == Player.TTarget7) || (ChatRoomCharacter[1].Name.toLowerCase() == Player.TTarget8) || (ChatRoomCharacter[1].Name.toLowerCase() == Player.TTarget9)){Player.Hypnotarget = ChatRoomCharacter[1]}}
    if ((ChatRoomCharacter[2]) && (ChatRoomCharacter[2] != Player)) {if ((ChatRoomCharacter[2].Name.toLowerCase() == Player.TTarget1) || (ChatRoomCharacter[2].Name.toLowerCase() == Player.TTarget2) || (ChatRoomCharacter[2].Name.toLowerCase() == Player.TTarget3) || (ChatRoomCharacter[2].Name.toLowerCase() == Player.TTarget4) || (ChatRoomCharacter[2].Name.toLowerCase() == Player.TTarget5) || (ChatRoomCharacter[2].Name.toLowerCase() == Player.TTarget6) || (ChatRoomCharacter[2].Name.toLowerCase() == Player.TTarget7) || (ChatRoomCharacter[2].Name.toLowerCase() == Player.TTarget8) || (ChatRoomCharacter[2].Name.toLowerCase() == Player.TTarget9)){Player.Hypnotarget = ChatRoomCharacter[2]}}
    if ((ChatRoomCharacter[3]) && (ChatRoomCharacter[3] != Player)) {if ((ChatRoomCharacter[3].Name.toLowerCase() == Player.TTarget1) || (ChatRoomCharacter[3].Name.toLowerCase() == Player.TTarget2) || (ChatRoomCharacter[3].Name.toLowerCase() == Player.TTarget3) || (ChatRoomCharacter[3].Name.toLowerCase() == Player.TTarget4) || (ChatRoomCharacter[3].Name.toLowerCase() == Player.TTarget5) || (ChatRoomCharacter[3].Name.toLowerCase() == Player.TTarget6) || (ChatRoomCharacter[3].Name.toLowerCase() == Player.TTarget7) || (ChatRoomCharacter[3].Name.toLowerCase() == Player.TTarget8) || (ChatRoomCharacter[3].Name.toLowerCase() == Player.TTarget9)){Player.Hypnotarget = ChatRoomCharacter[3]}}
    if ((ChatRoomCharacter[4]) && (ChatRoomCharacter[4] != Player)) {if ((ChatRoomCharacter[4].Name.toLowerCase() == Player.TTarget1) || (ChatRoomCharacter[4].Name.toLowerCase() == Player.TTarget2) || (ChatRoomCharacter[4].Name.toLowerCase() == Player.TTarget3) || (ChatRoomCharacter[4].Name.toLowerCase() == Player.TTarget4) || (ChatRoomCharacter[4].Name.toLowerCase() == Player.TTarget5) || (ChatRoomCharacter[4].Name.toLowerCase() == Player.TTarget6) || (ChatRoomCharacter[4].Name.toLowerCase() == Player.TTarget7) || (ChatRoomCharacter[4].Name.toLowerCase() == Player.TTarget8) || (ChatRoomCharacter[4].Name.toLowerCase() == Player.TTarget9)){Player.Hypnotarget = ChatRoomCharacter[4]}}
    if ((ChatRoomCharacter[5]) && (ChatRoomCharacter[5] != Player)) {if ((ChatRoomCharacter[5].Name.toLowerCase() == Player.TTarget1) || (ChatRoomCharacter[5].Name.toLowerCase() == Player.TTarget2) || (ChatRoomCharacter[5].Name.toLowerCase() == Player.TTarget3) || (ChatRoomCharacter[5].Name.toLowerCase() == Player.TTarget4) || (ChatRoomCharacter[5].Name.toLowerCase() == Player.TTarget5) || (ChatRoomCharacter[5].Name.toLowerCase() == Player.TTarget6) || (ChatRoomCharacter[5].Name.toLowerCase() == Player.TTarget7) || (ChatRoomCharacter[5].Name.toLowerCase() == Player.TTarget8) || (ChatRoomCharacter[5].Name.toLowerCase() == Player.TTarget9)){Player.Hypnotarget = ChatRoomCharacter[5]}}
    if ((ChatRoomCharacter[6]) && (ChatRoomCharacter[6] != Player)) {if ((ChatRoomCharacter[6].Name.toLowerCase() == Player.TTarget1) || (ChatRoomCharacter[6].Name.toLowerCase() == Player.TTarget2) || (ChatRoomCharacter[6].Name.toLowerCase() == Player.TTarget3) || (ChatRoomCharacter[6].Name.toLowerCase() == Player.TTarget4) || (ChatRoomCharacter[6].Name.toLowerCase() == Player.TTarget5) || (ChatRoomCharacter[6].Name.toLowerCase() == Player.TTarget6) || (ChatRoomCharacter[6].Name.toLowerCase() == Player.TTarget7) || (ChatRoomCharacter[6].Name.toLowerCase() == Player.TTarget8) || (ChatRoomCharacter[6].Name.toLowerCase() == Player.TTarget9)){Player.Hypnotarget = ChatRoomCharacter[6]}}
    if ((ChatRoomCharacter[7]) && (ChatRoomCharacter[7] != Player)) {if ((ChatRoomCharacter[7].Name.toLowerCase() == Player.TTarget1) || (ChatRoomCharacter[7].Name.toLowerCase() == Player.TTarget2) || (ChatRoomCharacter[7].Name.toLowerCase() == Player.TTarget3) || (ChatRoomCharacter[7].Name.toLowerCase() == Player.TTarget4) || (ChatRoomCharacter[7].Name.toLowerCase() == Player.TTarget5) || (ChatRoomCharacter[7].Name.toLowerCase() == Player.TTarget6) || (ChatRoomCharacter[7].Name.toLowerCase() == Player.TTarget7) || (ChatRoomCharacter[7].Name.toLowerCase() == Player.TTarget8) || (ChatRoomCharacter[7].Name.toLowerCase() == Player.TTarget9)){Player.Hypnotarget = ChatRoomCharacter[7]}}
    if ((ChatRoomCharacter[8]) && (ChatRoomCharacter[8] != Player)) {if ((ChatRoomCharacter[8].Name.toLowerCase() == Player.TTarget1) || (ChatRoomCharacter[8].Name.toLowerCase() == Player.TTarget2) || (ChatRoomCharacter[8].Name.toLowerCase() == Player.TTarget3) || (ChatRoomCharacter[8].Name.toLowerCase() == Player.TTarget4) || (ChatRoomCharacter[8].Name.toLowerCase() == Player.TTarget5) || (ChatRoomCharacter[8].Name.toLowerCase() == Player.TTarget6) || (ChatRoomCharacter[8].Name.toLowerCase() == Player.TTarget7) || (ChatRoomCharacter[8].Name.toLowerCase() == Player.TTarget8) || (ChatRoomCharacter[8].Name.toLowerCase() == Player.TTarget9)){Player.Hypnotarget = ChatRoomCharacter[8]}}
    if ((ChatRoomCharacter[9]) && (ChatRoomCharacter[9] != Player)) {if ((ChatRoomCharacter[9].Name.toLowerCase() == Player.TTarget1) || (ChatRoomCharacter[9].Name.toLowerCase() == Player.TTarget2) || (ChatRoomCharacter[9].Name.toLowerCase() == Player.TTarget3) || (ChatRoomCharacter[9].Name.toLowerCase() == Player.TTarget4) || (ChatRoomCharacter[9].Name.toLowerCase() == Player.TTarget5) || (ChatRoomCharacter[9].Name.toLowerCase() == Player.TTarget6) || (ChatRoomCharacter[9].Name.toLowerCase() == Player.TTarget7) || (ChatRoomCharacter[9].Name.toLowerCase() == Player.TTarget8) || (ChatRoomCharacter[9].Name.toLowerCase() == Player.TTarget9)){Player.Hypnotarget = ChatRoomCharacter[9]}}
    }
    if (((Player.Actvitytest != -01)&&(rpstype == "Chat"))){Player.Hypnotarget = Player.Hypnomaster; Player.Actvitytest = -01} 
    if (((Player.Actvitytest1 != -01)&&(rpstype == "Chat"))){Player.Hypnotarget = Player.Hypnomaster; Player.Actvitytest1 = -01} 
  
    if ((Player.Hypnoactivitytrigger == "on")) {
    Player.Hypnoactivitytrigger = "off";
  ActivityRunSelf(Player, Player.Hypnotarget, Player.Hypnoactivity);
  if (CurrentScreen == "ChatRoom") {
    var Dictionary = [];
    Dictionary.push({ Tag: "SourceCharacter", Text: Player.Name, MemberNumber: Player.MemberNumber });
    Dictionary.push({ Tag: "TargetCharacter", Text: Player.Hypnotarget.Name, MemberNumber: Player.Hypnotarget.MemberNumber });
    Dictionary.push({ Tag: "ActivityGroup", Text: Player.Hypnobodypart });
    Dictionary.push({ Tag: "ActivityName", Text: Player.HypnoAct });
    ServerSend("ChatRoomChat", { Content: ((Player.Hypnotarget.ID == 0) ? "ChatSelf-" : "ChatOther-") + Player.Hypnobodypart + "-" + Player.HypnoAct, Type: "Activity", Dictionary: Dictionary });
    if (Player.Hypnotarget.ID == 0 && Player.HypnoAct.indexOf("Struggle") >= 0 )
      ChatRoomStimulationMessage("StruggleAction")
            HypnoSpeachblocker = "on"; 
    DialogLeave(); Definations()
  }}
    else if (Player.Hypnoactivitytrigger != "on") {
        ActivityRunSelf(Player, Target, Activity);
        if (CurrentScreen == "ChatRoom") {
            var Dictionary = [];
            Dictionary.push({ Tag: "SourceCharacter", Text: Player.Name, MemberNumber: Player.MemberNumber });
            Dictionary.push({ Tag: "TargetCharacter", Text: C.Name, MemberNumber: C.MemberNumber });
            Dictionary.push({ Tag: "ActivityGroup", Text: C.FocusGroup.Name });
            Dictionary.push({ Tag: "ActivityName", Text: Activity.Name });
            ServerSend("ChatRoomChat", { Content: ((C.ID == 0) ? "ChatSelf-" : "ChatOther-") + C.FocusGroup.Name + "-" + Activity.Name, Type: "Activity", Dictionary: Dictionary });
            if (C.ID == 0 && Activity.Name.indexOf("Struggle") >= 0 )
                ChatRoomStimulationMessage("StruggleAction")
            DialogLeave();
        }}
  }
  //---------------------------------------------------------------------------------------------------------------------------------------------------
  function ActivityRunSelf(Source, Target, Activity) {
    if ((Player.Hypnoactivitytrigger == "on")) {
  if (((Player.ArousalSettings.Active == "Hybrid") || (Player.ArousalSettings.Active == "Automatic")) && (Player.ID == 0) && (Player.Hypnotarget.ID != 0)) {
    var Factor = (PreferenceGetActivityFactor(Player, Player.HypnoAct, false) * 5) - 10; // Check how much the player likes the activity, from -10 to +10              
    Factor = Factor + Math.floor((Math.random() * 8)); // Random 0 to 7 bonus
    if (Player.Hypnotarget.IsLoverOfPlayer()) Factor = Factor + Math.floor((Math.random() * 8)); // Another random 0 to 7 bonus if the target is the player's lover
    ActivitySetArousalTimer(Player, Player.Hypnoactivity, "ActivityOnOther", Factor); // For activities on other, it cannot go over 2/3
  }}
    else if (Player.Hypnoactivitytrigger != "on") {
        if (((Player.ArousalSettings.Active == "Hybrid") || (Player.ArousalSettings.Active == "Automatic")) && (Source.ID == 0) && (Target.ID != 0)) {
            var Factor = (PreferenceGetActivityFactor(Player, Activity.Name, false) * 5) - 10; // Check how much the player likes the activity, from -10 to +10
            Factor = Factor + Math.floor((Math.random() * 8)); // Random 0 to 7 bonus
            if (Target.IsLoverOfPlayer()) Factor = Factor + Math.floor((Math.random() * 8)); // Another random 0 to 7 bonus if the target is the player's lover
            ActivitySetArousalTimer(Player, Activity, "ActivityOnOther", Factor); // For activities on other, it cannot go over 2/3
        }}
  }
  /*---------------------------------------------------------------------------------------------------------------------------------------------------
  
  */
  function Definations(){
        if (Placeholder == "off") {
            Letter = Player.Emote.split(' ')
            Player.Eersi = Letter[0]
            Player.Tailsi = Letter[1]
            Player.Wingsi = Letter[2]
            Player.Blushi = Letter[3]
            Player.Brows = Letter[4]
            Player.Emoti = Letter[5]
            Player.Eye = Letter[6]
            Player.Fluid = Letter[7]
            Player.Mouthi = Letter[8]
            Player.Arms = Letter[9]
            Player.Legs = Letter[10]
        }
        if((Player.Wingsset1 != "non")){
            if(Player.Wingsi > 0){	
                if(Player.Wingsi == 1){	InventoryWear(Player, Player.Wingsset1, "Wings", Wingcolor)}	
                if(Player.Wingsi == 2){	InventoryWear(Player, Player.Wingsset2, "Wings", Wingcolor)}	
                if(Player.Wingsi == 3){	InventoryRemove(Player, "Wings") }
            }
        }
        if((Player.Tailset1 != "non")){
            if(Player.Tailsi > 0){	
                if(Player.Tailsi  == 1){InventoryWear(Player, Player.Tailset1, "TailStraps", Tailcolor)}	
                if(Player.Tailsi  == 2){InventoryWear(Player, Player.Tailset2, "TailStraps", Tailcolor)}
                if(Player.Tailsi  == 3){InventoryRemove(Player, "TailStraps") }
            }
        }
        if((Player.Earset1 != "non")){
            if(Player.Eersi > 0){	
                if(Player.Eersi == 1){	InventoryWear(Player, Player.Earset1, "HairAccessory2", Earcolor)}	
                if(Player.Eersi == 2){	InventoryWear(Player, Player.Earset2, "HairAccessory2", Earcolor)}
                if(Player.Eersi == 3){	InventoryRemove(Player, "HairAccessory2") }
            }
        }
        if((Player.Blushi != "non")){
            if(Player.Blushi > 0){
                if(Player.Blushi  == 1){CharacterSetFacialExpression(Player, "Blush", null)}
                if(Player.Blushi  == 2){CharacterSetFacialExpression(Player, "Blush", "Low")}
                if(Player.Blushi  == 3){CharacterSetFacialExpression(Player, "Blush", "Medium")}
                if(Player.Blushi  == 4){CharacterSetFacialExpression(Player, "Blush", "High")}
                if(Player.Blushi  == 5){CharacterSetFacialExpression(Player, "Blush", "VeryHigh")}
                if(Player.Blushi  == 6){CharacterSetFacialExpression(Player, "Blush", "Extreme")}
                if(Player.Blushi  == 7){CharacterSetFacialExpression(Player, "Blush", "ShortBreath")}
            }
        }
        if((Player.Brows != "non")){
            if(Player.Brows > 0){
                if(Player.Brows == 1){CharacterSetFacialExpression(Player, "Eyebrows", null)}
                if(Player.Brows == 2){CharacterSetFacialExpression(Player, "Eyebrows", "Raised")}
                if(Player.Brows == 3){CharacterSetFacialExpression(Player, "Eyebrows", "Lowered")}
                if(Player.Brows == 4){CharacterSetFacialExpression(Player, "Eyebrows", "OneRaised")}
                if(Player.Brows == 5){CharacterSetFacialExpression(Player, "Eyebrows", "Harsh")}
                if(Player.Brows == 6){CharacterSetFacialExpression(Player, "Eyebrows", "Angry")}
                if(Player.Brows == 7){CharacterSetFacialExpression(Player, "BrEyebrowsows", "Soft")}
            }
        }
        if((Player.Emoti != "non")){
            if(Player.Emoti > 0){
                if(Player.Emoti == 1){CharacterSetFacialExpression(Player, "Emoticon", null)}
                if(Player.Emoti == 2){CharacterSetFacialExpression(Player, "Emoticon", "Afk")}
                if(Player.Emoti == 3){CharacterSetFacialExpression(Player, "Emoticon", "Whisper")}
                if(Player.Emoti == 4){CharacterSetFacialExpression(Player, "Emoticon", "Sleep")}
                if(Player.Emoti == 5){CharacterSetFacialExpression(Player, "Emoticon", "Hearts")}
                if(Player.Emoti == 6){CharacterSetFacialExpression(Player, "Emoticon", "Tear")}
                if(Player.Emoti == 7){CharacterSetFacialExpression(Player, "Emoticon", "Hearing")}
                if(Player.Emoti == 8){CharacterSetFacialExpression(Player, "Emoticon", "Confusing")}
                if(Player.Emoti == 9){CharacterSetFacialExpression(Player, "Emoticon", "Exclamation")}
                if(Player.Emoti == 10){CharacterSetFacialExpression(Player, "Emoticon", "Annoyed")}
                if(Player.Emoti == 11){CharacterSetFacialExpression(Player, "Emoticon", "Read")}
                if(Player.Emoti == 12){CharacterSetFacialExpression(Player, "Emoticon", "RaisedHand")}
                if(Player.Emoti == 13){CharacterSetFacialExpression(Player, "Emoticon", "Spectator")}
                if(Player.Emoti == 14){CharacterSetFacialExpression(Player, "Emoticon", "ThumbsDown")}
                if(Player.Emoti == 15){CharacterSetFacialExpression(Player, "Emoticon", "ThumbsUp")}
                if(Player.Emoti == 16){CharacterSetFacialExpression(Player, "Emoticon", "LoveRope")}
                if(Player.Emoti == 17){CharacterSetFacialExpression(Player, "Emoticon", "LoveGag")}
                if(Player.Emoti == 18){CharacterSetFacialExpression(Player, "Emoticon", "LoveLocks")}
                if(Player.Emoti == 19){CharacterSetFacialExpression(Player, "Emoticon", "Wardrobe")}
            }
        }
        if((Player.Eye != "non")){
            if(Player.Eye > 0){
                if(Player.Eye == 1){CharacterSetFacialExpression(Player, "Eyes", null)}
                if(Player.Eye == 2){CharacterSetFacialExpression(Player, "Eyes", "Closed")}
                if(Player.Eye == 3){CharacterSetFacialExpression(Player, "Eyes", "Dazed")}
                if(Player.Eye == 4){CharacterSetFacialExpression(Player, "Eyes", "Shy")}
                if(Player.Eye == 5){CharacterSetFacialExpression(Player, "Eyes", "Sad")}
                if(Player.Eye == 6){CharacterSetFacialExpression(Player, "Eyes", "Horny")}
                if(Player.Eye == 7){CharacterSetFacialExpression(Player, "Eyes", "Lewd")}
                if(Player.Eye == 8){CharacterSetFacialExpression(Player, "Eyes", "VeryLewd")}
                if(Player.Eye == 9){CharacterSetFacialExpression(Player, "Eyes", "Heart")}
                if(Player.Eye == 10){CharacterSetFacialExpression(Player, "Eyes", "HeartPink")}
                if(Player.Eye == 11){CharacterSetFacialExpression(Player, "Eyes", "LewdHeart")}
                if(Player.Eye == 12){CharacterSetFacialExpression(Player, "Eyes", "LewdHeartPink")}
                if(Player.Eye == 13){CharacterSetFacialExpression(Player, "Eyes", "Dizzy")}
                if(Player.Eye == 14){CharacterSetFacialExpression(Player, "Eyes", "Daydream")}
                if(Player.Eye == 15){CharacterSetFacialExpression(Player, "Eyes", "Angry")}
                if(Player.Eye == 16){CharacterSetFacialExpression(Player, "Eyes", "Surprised")}
                if(Player.Eye == 17){CharacterSetFacialExpression(Player, "Eyes", "Scared")}
            }
        }
        if((Player.Fluid != "non")){
            if(Player.Fluid > 0){
                if(Player.Fluid == 1){CharacterSetFacialExpression(Player, "Fluids", null)}
                if(Player.Fluid == 2){CharacterSetFacialExpression(Player, "Fluids", "DroolLow")}
                if(Player.Fluid == 3){CharacterSetFacialExpression(Player, "Fluids", "DroolMedium")}
                if(Player.Fluid == 4){CharacterSetFacialExpression(Player, "Fluids", "DroolHigh")}
                if(Player.Fluid == 5){CharacterSetFacialExpression(Player, "Fluids", "DroolSides")}
                if(Player.Fluid == 6){CharacterSetFacialExpression(Player, "Fluids", "DroolMessy")}
                if(Player.Fluid == 7){CharacterSetFacialExpression(Player, "Fluids", "DroolTearsLow")}
                if(Player.Fluid == 8){CharacterSetFacialExpression(Player, "Fluids", "DroolTearsMedium")}
                if(Player.Fluid == 9){CharacterSetFacialExpression(Player, "Fluids", "DroolTearsHigh")}
                if(Player.Fluid == 10){CharacterSetFacialExpression(Player, "Fluids", "DroolTearsMessy")}
                if(Player.Fluid == 11){CharacterSetFacialExpression(Player, "Fluids", "DroolTearsSides")}
                if(Player.Fluid == 12){CharacterSetFacialExpression(Player, "Fluids", "TearsHigh")}
                if(Player.Fluid == 13){CharacterSetFacialExpression(Player, "Fluids", "TearsMedium")}
                if(Player.Fluid == 14){CharacterSetFacialExpression(Player, "Fluids", "TearsLow")}
            }
        }
        if((Player.Mouthi != "non")){
            if(Player.Mouthi > 0){
                if(Player.Mouthi == 1){CharacterSetFacialExpression(Player, "Mouth", null)}
                if(Player.Mouthi == 2){CharacterSetFacialExpression(Player, "Mouth", "Frown")}
                if(Player.Mouthi == 3){CharacterSetFacialExpression(Player, "Mouth", "Sad")}
                if(Player.Mouthi == 4){CharacterSetFacialExpression(Player, "Mouth", "Pained")}
                if(Player.Mouthi == 5){CharacterSetFacialExpression(Player, "Mouth", "Angry")}
                if(Player.Mouthi == 6){CharacterSetFacialExpression(Player, "Mouth", "HalfOpen")}
                if(Player.Mouthi == 7){CharacterSetFacialExpression(Player, "Mouth", "Open")}
                if(Player.Mouthi == 8){CharacterSetFacialExpression(Player, "Mouth", "Ahegao")}
                if(Player.Mouthi == 9){CharacterSetFacialExpression(Player, "Mouth", "Moan")}
                if(Player.Mouthi == 10){CharacterSetFacialExpression(Player, "Mouth", "TonguePinch")}
                if(Player.Mouthi == 11){CharacterSetFacialExpression(Player, "Mouth", "LipBite")}
                if(Player.Mouthi == 12){CharacterSetFacialExpression(Player, "Mouth", "Happy")}
                if(Player.Mouthi == 13){CharacterSetFacialExpression(Player, "Mouth", "Devious")}
                if(Player.Mouthi == 14){CharacterSetFacialExpression(Player, "Mouth", "Laughing")}
                if(Player.Mouthi == 15){CharacterSetFacialExpression(Player, "Mouth", "Grin")}
                if(Player.Mouthi == 16){CharacterSetFacialExpression(Player, "Mouth", "Smirk")}
                if(Player.Mouthi == 17){CharacterSetFacialExpression(Player, "Mouth", "Pout")}
            }
        }
        if((Player.Arms != "non")){
            if(Player.Arms > 0){
                if( !InventoryGet(Player, "ItemArms")){
                    if(Player.Arms == 1){    CharacterSetActivePose(Player, "BaseUpper")    }
                    if(Player.Arms == 2){    CharacterSetActivePose(Player, "Yoked")    }
                    if(Player.Arms == 3){    CharacterSetActivePose(Player, "OverTheHead")    }
                    if(Player.Arms == 4){    CharacterSetActivePose(Player, "BackBoxTie")    }
                    if(Player.Arms == 5){    CharacterSetActivePose(Player, "BackElbowTouch")    }
                    if(Player.Arms == 6){    CharacterSetActivePose(Player, "BackCuffs")    }
                }
            }
        }
        if((Player.Legs != "non")){
            if(Player.Legs > 0){
                if((!InventoryGet(Player, "ItemDevices")) || ( !InventoryGet(Player, "ItemFeet")) || ( !InventoryGet(Player, "ItemLegs")) || ( !InventoryGet(Player, "ItemArms"))){
                    if(Player.Legs == 1){    CharacterSetActivePose(Player, "LegsOpen")    }
                    if(Player.Legs == 2){    CharacterSetActivePose(Player, "Kneel")    }
                    if(Player.Legs == 3){    CharacterSetActivePose(Player, "LegsClosed")    }
                    if(Player.Legs == 4){    CharacterSetActivePose(Player, "KneelingSpread")    }
                    if(Player.Legs == 5){    CharacterSetActivePose(Player, "AllFours")    }
                }
            }
        }
  if (Player.HypnoSpeachblockertrigger == "on") {	
    Player.HypnoSpeachblocker = "on";}
  CharacterRefresh(Player)
  ChatRoomCharacterUpdate(Player)	
  ChatRoomMessage()
  }
  /*---------------------------------------------------------------------------------------------------------------------------------------------------
  Player.EmoteBase 
  */
  function RPSSetup(){
  if(Placeholder == "off"){               //XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX  Basic Scan  XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX 
  if ((Player.Hypno != "on")){
    if(Placeholder == "off"){           //-------------------------------Mistress Scan ------------------------------------     Player.RepD
        for(var R=0;R< Player.Reputation.length;R++)	
            if( Player.Reputation[R].Type=="Dominant"){if( Player.Reputation[R].Value>=1){Player.RPSRepD = "Misstres"} else {Player.RPSRepD = "Sub"}}
        }
    if(Placeholder == "off"){           //-------------------------------Nurse Scan    ------------------------------------     Player.RepA
        for(var R=0;R< Player.Reputation.length;R++)		
            if( Player.Reputation[R].Type=="Asylum"){if( Player.Reputation[R].Value>=1){Player.RepA = "Nurse"} else {Player.RepA = "Patient"}}
        }
    if(Placeholder == "off"){           //-------------------------------Ears Scan  ------------------------------------
        if(InventoryGet( Player, "HairAccessory2")){
                 if(InventoryGet( Player, "HairAccessory2").Asset.Name=="BunnyEars1"){ Player.Earset1 = "BunnyEars1";  Player.Earset2 = "BunnyEars1"}
            else if(InventoryGet( Player, "HairAccessory2").Asset.Name=="BunnyEars2"){ Player.Earset1 = "BunnyEars2";  Player.Earset2 = "BunnyEars2"}
            else if(InventoryGet( Player, "HairAccessory2").Asset.Name=="FoxEars1"){ Player.Earset1 = "FoxEars1";  Player.Earset2 = "PuppyEars2"}
            else if(InventoryGet( Player, "HairAccessory2").Asset.Name=="FoxEars2"){ Player.Earset1 = "FoxEars2";  Player.Earset2 = "PuppyEars2"}
            else if(InventoryGet( Player, "HairAccessory2").Asset.Name=="FoxEars3"){ Player.Earset1 = "FoxEars3";  Player.Earset2 = "WolfEars2"}
            else if(InventoryGet( Player, "HairAccessory2").Asset.Name=="Ears1"){ Player.Earset1 = "Ears1";  Player.Earset2 = "PuppyEars1"}
            else if(InventoryGet( Player, "HairAccessory2").Asset.Name=="Ears2"){ Player.Earset1 = "Ears2";  Player.Earset2 = "PuppyEars2"}
            else if(InventoryGet( Player, "HairAccessory2").Asset.Name=="KittenEars1"){ Player.Earset1 = "KittenEars1";  Player.Earset2 = "PuppyEars2"}
            else if(InventoryGet( Player, "HairAccessory2").Asset.Name=="KittenEars2"){ Player.Earset1 = "KittenEars2";  Player.Earset2 = "WolfEars2"}
            else if(InventoryGet( Player, "HairAccessory2").Asset.Name=="MouseEars1"){ Player.Earset1 = "MouseEars1";  Player.Earset2 = "MouseEars2"}
            else if(InventoryGet( Player, "HairAccessory2").Asset.Name=="MouseEars2"){ Player.Earset1 = "MouseEars2";  Player.Earset2 = "MouseEars1"}
            else if(InventoryGet( Player, "HairAccessory2").Asset.Name=="PonyEars1"){ Player.Earset1 = "PonyEars1";  Player.Earset2 = "PonyEars1"}
            else if(InventoryGet( Player, "HairAccessory2").Asset.Name=="PuppyEars1"){ Player.Earset1 = "PuppyEars1";  Player.Earset2 = "PuppyEars2"}
            else if(InventoryGet( Player, "HairAccessory2").Asset.Name=="PuppyEars2"){ Player.Earset1 = "PuppyEars2";  Player.Earset2 = "WolfEars1"}
            else if(InventoryGet( Player, "HairAccessory2").Asset.Name=="RaccoonEars1"){ Player.Earset1 = "RaccoonEars1";  Player.Earset2 = "RaccoonEars1"}
            else if(InventoryGet( Player, "HairAccessory2").Asset.Name=="WolfEars1"){ Player.Earset1 = "WolfEars1";  Player.Earset2 = "PuppyEars2"}
            else if(InventoryGet( Player, "HairAccessory2").Asset.Name=="WolfEars2"){ Player.Earset1 = "WolfEars2";  Player.Earset2 = "KittenEars2"}
            else if(InventoryGet( Player, "HairAccessory2").Asset.Name=="CowHorns"){ Player.Earset1 = "CowHorns";  Player.Earset2 = "CowHorns"}
            else if(InventoryGet( Player, "HairAccessory2").Asset.Name=="SuccubusHorns"){ Player.Earset1 = "SuccubusHorns";  Player.Earset2 = "SuccubusHorns"}
            else if(InventoryGet( Player, "HairAccessory2").Asset.Name=="Horns"){ Player.Earset1 = "Horns";  Player.Earset2 = "Horns"}
            else if(InventoryGet( Player, "HairAccessory2").Asset.Name=="Horns2"){ Player.Earset1 = "Horns2";  Player.Earset2 = "Horns2"}
            else if(InventoryGet( Player, "HairAccessory2").Asset.Name=="Horns3"){ Player.Earset1 = "Horns3";  Player.Earset2 = "Horns3"}
            else if(InventoryGet( Player, "HairAccessory2").Asset.Name=="BatWings"){ Player.Earset1 = "BatWings";  Player.Earset2 = "BatWings"}
            else if(InventoryGet( Player, "HairAccessory2").Asset.Name=="ElfEars"){ Player.Earset1 = "ElfEars";  Player.Earset2 = "ElfEars"}
        }
        else{
             Player.Earset1 = "non"
             Player.Earset2 = "non"
        }}
    if(Placeholder == "off"){           //-------------------------------Tail Scan  ------------------------------------
        if(InventoryGet( Player, "TailStraps")){//-------------------------------Tail Scan  ------------------------------------
            if(InventoryGet( Player, "TailStraps").Asset.Name=="BunnyTailStrap"){ Player.Tailset1 = "BunnyTailStrap";  Player.Tailset2 = "BunnyTailStrap"}
            else if(InventoryGet( Player, "TailStraps").Asset.Name=="CowtailStrap"){ Player.Tailset1 = "CowtailStrap";  Player.Tailset2 = "CowtailStrap"}
            else if(InventoryGet( Player, "TailStraps").Asset.Name=="DemonPlug"){ Player.Tailset1 = "DemonPlug";  Player.Tailset2 = "DemonPlug"}
            else if(InventoryGet( Player, "TailStraps").Asset.Name=="FoxTailStrap1"){ Player.Tailset1 = "FoxTailStrap1";  Player.Tailset2 = "FoxTailStrap2"}
            else if(InventoryGet( Player, "TailStraps").Asset.Name=="FoxTailStrap2"){ Player.Tailset1 = "FoxTailStrap2";  Player.Tailset2 = "FoxTailStrap1"}
            else if(InventoryGet( Player, "TailStraps").Asset.Name=="FoxTailsStrap"){ Player.Tailset1 = "FoxTailsStrap";  Player.Tailset2 = "FoxTailsStrap"}
