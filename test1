      else if (InventoryGet(Player, "Eyes").Property.Expression=="Daydream"){Player.EyesBase = "14"}
      else if (InventoryGet(Player, "Eyes").Property.Expression=="Angry"){Player.EyesBase = "15"}
      else if (InventoryGet(Player, "Eyes").Property.Expression=="Surprised"){Player.EyesBase = "16"}
      else if (InventoryGet(Player, "Eyes").Property.Expression=="Scared"){Player.EyesBase = "17"}
        else {Player.EyesBase = "1"}
        }
    if(Placeholder == "off"){           //-------------------------------mouth Scan  ------------------------------------
      if (InventoryGet(Player, "Mouth").Property.Expression=="Frown"){Player.MouthBase = "2"}
      else if (InventoryGet(Player, "Mouth").Property.Expression=="Sad"){Player.MouthBase = "3"}
      else if (InventoryGet(Player, "Mouth").Property.Expression=="Pained"){Player.MouthBase = "4"}
      else if (InventoryGet(Player, "Mouth").Property.Expression=="Angry"){Player.MouthBase = "5"}
      else if (InventoryGet(Player, "Mouth").Property.Expression=="HalfOpen"){Player.MouthBase = "6"}
      else if (InventoryGet(Player, "Mouth").Property.Expression=="Open"){Player.MouthBase = "7"}
      else if (InventoryGet(Player, "Mouth").Property.Expression=="Ahegao"){Player.MouthBase = "8"}
      else if (InventoryGet(Player, "Mouth").Property.Expression=="Moan"){Player.MouthBase = "9"}
      else if (InventoryGet(Player, "Mouth").Property.Expression=="TonguePinch"){Player.MouthBase = "10"}
      else if (InventoryGet(Player, "Mouth").Property.Expression=="LipBite"){Player.MouthBase = "11"}
      else if (InventoryGet(Player, "Mouth").Property.Expression=="Happy"){Player.MouthBase = "12"}
      else if (InventoryGet(Player, "Mouth").Property.Expression=="Devious"){Player.MouthBase = "13"}
      else if (InventoryGet(Player, "Mouth").Property.Expression=="Laughing"){Player.MouthBase = "14"}
      else if (InventoryGet(Player, "Mouth").Property.Expression=="Grin"){Player.MouthBase = "15"}
      else if (InventoryGet(Player, "Mouth").Property.Expression=="Smirk"){Player.MouthBase = "16"}
        else {Player.MouthBase = "1"}
        }
  if(Placeholder == "off"){ 
    HairFrontIndex=Player.Appearance.findIndex(it=>it.Asset.Name.includes("HairFront"));
