    else if (Player.Stage == 13){Player.Emote = "00 00 00 00 00 00 00 00 00 00 00"; Player.Looptimer = 100; Player.HypnoSpeachblocker = "off";InventoryGet(Player, "ItemMouth").Property = {Type: "Double"}}
    else if (Player.Stage == 14){Player.Emote = "00 00 00 00 00 00 00 00 00 00 00"; Player.Looptimer = 100; Player.HypnoSpeachblocker = "off";InventoryGet(Player, "ItemMouth").Property = {Type: "Cover"}}
