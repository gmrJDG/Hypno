  function RPSBasics(){
    Placeholder = "off"; Vict = Player; Player.RPSScriptstatus = "Start"; Player.RPSTrigger = 00; Player.RPSTrigger = "!Startscript"; Player.Commandblocker = "Offline"
  Player.RPSTriggerani = 00; Player.RPSTriggeremo = 00; Player.Stage = 00; Player.RPStarget = 00000; Player.RPSTriggerrea = 00; Player.Status = "Awake"; Player.SavetyLoop = "inactive";
    Player.RPSEmotesetup = "Online"; Player.Fontsize = "Normal";
    Player.RPSBreathplaysetup = "Online"; 	Player.RPSBreathplaytimer = 90; Player.Noseblocked = 0; Player.Mouthblocked = 0; Player.breathplay = "off"; Player.RPSTriggerbpl = 00; Player.breathplay = "off"
                      Player.Nosepinch = 30; Player.Handgag = 30;
    Player.RPSChloroformsetup = "Online"; Player.RPSChloroformtimer = 12; Player.RPSTriggerchl = 00
  
    Player.RPSHypnosissetup   = "Online"; Player.Savetytimer = 20; Player.rpsHypnoticker = 120; Player.Hypnotrigger = "Mango"; Player.Hypnomaster = Player; Player.Hypno = "off"; Player.HypnoSpeachblocker = "on"
  Player.HypnoNaughty = "Online"; Player.HypnoHumilation = "Online"; Player.HypnoPetplay = "Online"; Player.HypnoSpeachblocker = "off"; Player.RPSTriggerhyp = 0; Player.RPSTriggerhypn = 00; 
  Player.RPSTriggerhyph = 00; Player.RPSTriggerhypp = 00
  Player.Hypnotarget = Player; Player.Hypnoactivity = 000; Player.Hypnoactivitytrigger = "off"; Player.Hypnoloop == "off"
  
  
  
  CharacterSetFacialExpression(Player, "Blush", "Low")
  CharacterSetFacialExpression(Player, "Emoticon", "Confusing")
  CharacterRefresh(Player)
  ChatRoomCharacterUpdate(Player)	
  CharacterSetFacialExpression(Player, "Emoticon", null)
  CharacterRefresh(Player)
  ChatRoomCharacterUpdate(Player)	
  delete InventoryGet(Player, 'Emoticon').Property.OverrideHeight;
    CurrentScreen === 'ChatRoom';
    CharacterRefresh(Player)
    ChatRoomCharacterUpdate(Player)	
  RPSSetup()
  }
  RPSBasics()
