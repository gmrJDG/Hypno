            else if(InventoryGet( Player, "TailStraps").Asset.Name=="KittenTailStrap1"){ Player.Tailset1 = "KittenTailStrap1";  Player.Tailset2 = "MouseTailStrap2"}
            else if(InventoryGet( Player, "TailStraps").Asset.Name=="KittenTailStrap2"){ Player.Tailset1 = "KittenTailStrap2";  Player.Tailset2 = "WolfTailStrap3"}
            else if(InventoryGet( Player, "TailStraps").Asset.Name=="MouseTailStrap1"){ Player.Tailset1 = "MouseTailStrap1";  Player.Tailset2 = "MouseTailStrap2"}
            else if(InventoryGet( Player, "TailStraps").Asset.Name=="MouseTailStrap2"){ Player.Tailset1 = "MouseTailStrap2";  Player.Tailset2 = "MouseTailStrap1"}
            else if(InventoryGet( Player, "TailStraps").Asset.Name=="PuppyTailStrap"){ Player.Tailset1 = "PuppyTailStrap";  Player.Tailset2 = "PuppyTailStrap1"}
            else if(InventoryGet( Player, "TailStraps").Asset.Name=="PuppyTailStrap1"){ Player.Tailset1 = "PuppyTailStrap1";  Player.Tailset2 = "WolfTailStrap2"}
            else if(InventoryGet( Player, "TailStraps").Asset.Name=="RaccoonStrap"){ Player.Tailset1 = "RaccoonStrap";  Player.Tailset2 = "RaccoonTailStrap"}
